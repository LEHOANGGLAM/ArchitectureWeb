@{
    var Setting = (List<BlueDataWeb.Models.Entites.SettingDB>)ViewBag.lstSetting;

    var Zalo = Setting.Where(x => x.Ma == "ZALO").FirstOrDefault() != null ? Setting.Where(x => x.Ma == "ZALO").FirstOrDefault().Value : "";
    var GOOGLE_SITE_VERIFICATION = Setting.Where(x => x.Ma == "GOOGLE_SITE_VERIFICATION").FirstOrDefault() != null ? Setting.Where(x => x.Ma == "GOOGLE_SITE_VERIFICATION").FirstOrDefault().Value : "";

}

<!doctype html>
<html lang="vi" itemscope itemtype="http://schema.org/WebSite">
<head>
    <meta charset="utf-8">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="SHORTCUT ICON" href="~/Themes/easyweb/images/ICONPAGE.PNG" type="image/x-icon" />
    <meta name="author" content="CÔNG TY CỔ PHẦN TM CN HÒA PHÁT MIỀN NAM" />
    <meta name="copyright" content="CÔNG TY CỔ PHẦN TM CN HÒA PHÁT MIỀN NAM" />
    <title> @Html.Raw(HttpUtility.HtmlDecode(ViewBag.SEO_TITLE))</title>

    <meta name="description"    @Html.Raw("content=\"" + ViewBag.SEO_DES + "\"") />
    <meta name="DC.title"    @Html.Raw("content=\"" + ViewBag.SEO_TITLE + "\"") />
    @*<meta name="geo.region" content="VN" />
        <meta name="geo.placename" content="***" />
        <meta name="geo.position" content="***" />
        <meta name="ICBM" content="***" />
        <meta name="DC.identifier" content="" />*@
    <meta property="og:image" content="@ViewBag.SEO_IMG" />
    <meta property="og:title" @Html.Raw("content=\"" + ViewBag.SEO_TITLE + "\"") />
    <meta property="og:url" content="@Request.Url.AbsoluteUri" />
    <meta property="og:locale" content="vi_VN" />

    <link rel="canonical" href="@Request.Url.AbsoluteUri" />
    <meta property="og:site_name" content="CÔNG TY CỔ PHẦN TM CN HÒA PHÁT MIỀN NAM" />
    <meta property="og:description" @Html.Raw("content=\"" + ViewBag.SEO_DES + "\"") />
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="CÔNG TY CỔ PHẦN TM CN HÒA PHÁT MIỀN NAM" />

    <meta name="google-site-verification" content="@GOOGLE_SITE_VERIFICATION" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500,700&display=swap&subset=vietnamese" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
 
    <link href="~/Themes/easyweb/css/owlcarousel/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/Themes/easyweb/css/owlcarousel/owl.carousel.min.css" rel="stylesheet" />
    <script src="~/Themes/easyweb/css/js/jquery.min.js"></script>
    <script src="~/Themes/easyweb/css/js/intersection-observer.min.js"></script> 
    <script src="~/Themes/easyweb/css/js/lazyload.min.js"></script>
    <script src="~/Themes/easyweb/css/js/list.min.js"></script>
    <script src="~/Themes/easyweb/css/owlcarousel/owl.carousel.min.js"></script>

    <link href="~/Themes/easyweb/css/customcss.css" rel="stylesheet" />
</head>
<body>

    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v13.0&appId=935830533125979&autoLogAppEvents=1" nonce="qy9DtYv9"></script>


    <div id="wapper">
        <a class="zalo_icon_fixed" style="position: fixed; right: 0; bottom: 120px; z-index: 999; display: block; width: 70px; height: 70px;" href="https://zalo.me/@Zalo" target="_blank">
            <div style="position:absolute; width: 100%; height: 100%; top: 0; left: 0;"></div>
            <iframe src="https://sp.zalo.me/plugins/chat-widget?position=null&oaid=4462152339089565647" frameborder="0" width="70" height="70"></iframe>
        </a>
        @Html.Partial("_Header")
        @Html.Partial("_ArchitectureMenu")
        @RenderBody()

        @Html.Partial("_Footer")

        <!--JavaScript at end of body for optimized loading-->
    </div>

    <h1 style="position:absolute; top:-1000px;"></h1>
    <h2 style="position:absolute; top:-1000px;"></h2>
    <h3 style="position:absolute; top:-1000px;"></h3>
    <div id="bttop"></div>
    <div class="popup_dutoan_blank">
        <div class="popup_dutoan_content">
            <button type="button" class="close_popup"></button>
            <div class="popup_dutoan_content1"></div>
        </div>
    </div>

    <script src="~/Themes/easyweb/css/js/global.js"></script>
    <script src="~/Themes/easyweb/css/js/wow.min.js"></script> 
    <script src="~/Themes/easyweb/css/js/sweetalert.min.js"></script>
    <script src="~/Themes/easyweb/css/js/materialize.min.js"></script>
    <script src="~/Themes/easyweb/css/js/slick.min.js"></script>
    <script>
        new WOW().init();
    </script>
 

    @RenderSection("scripts", required: false)

</body>
</html>